{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "AccountResponse",
  "type": "object",
  "properties": {
    "gmail_address": {
      "type": "string"
    },
    "google_id": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "registered_date": {
      "type": "string"
    },
    "user_id": {
      "type": "integer",
      "format": "int32"
    },
    "visibility": {
      "type": "boolean"
    }
  },
  "required": [
    "user_id",
    "google_id",
    "gmail_address",
    "name",
    "registered_date",
    "visibility"
  ]
}